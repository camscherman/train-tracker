{
  "name": "traintracker",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "nodemon ./bin/www",
    "debug": "nodemon --inspect ./bin/www",
    "db:create": "createdb --echo trainTracker_dev",
    "db:drop": "dropdb --echo --if-exists trainTracker_dev",
    "db:migrate": "knex migrate:latest"
  },
  "dependencies": {
    "body-parser": "~1.18.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.6.9",
    "ejs": "~2.5.7",
    "express": "~4.15.5",
    "knex": "^0.14.2",
    "kx": "0.0.2",
    "morgan": "~1.9.0",
    "node-fetch": "^1.7.3",
    "pg": "^7.4.0",
    "serve-favicon": "~2.4.5"
  },
  "devDependencies": {
    "nodemon": "^1.14.6"
  }
}
